<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

  <!-- Servlets -->
  <servlet>
    <servlet-name>postServlet</servlet-name>
    <servlet-class>migool.op.server.PostServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>postServlet</servlet-name>
    <url-pattern>/online_poster_gwt/post</url-pattern>
  </servlet-mapping>
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>Online_poster_gwt.html</welcome-file>
  </welcome-file-list>

</web-app>
